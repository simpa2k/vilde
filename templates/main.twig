<!DOCTYPE html>
<html>
	<head>
		<title>Vilde</title>

		<link rel="stylesheet" type="text/css" href="css/main.css">
        <link rel="stylesheet" type="text/css" href="css/bootstrap/css/bootstrap.min.css">

		<meta name="viewport" content="width=device-width, initial-scale=1.0">
	</head>
	<body>
	
		<div id="background">
			<img id="logo" src="images/logga.png">
		</div>

		{% include 'customnavbar.twig' %}

		<div id="main">

			{% include 'aboutandquotes.twig' %}

			{% include 'members.twig' %}

			{% include 'newsandgigs.twig' %}


			<div id="quote-section">
				<div class="container-fluid">
					<div class="row">
						<blockquote>
							<p>{{ quotes[0].review }}</p>
							<footer>{{ quotes[0].info }}</footer>
						</blockquote>
					</div>
				</div>
			</div>

			{% include 'media.twig' %}

			<div id="big-image"></div>

			{% include 'contact.twig' %}

			{% include 'footer.twig' %}
			
		</div>

		<script type="text/javascript" src="js/main.js"></script>
		<script type="text/javascript" src="js/jquery.js"></script>
		<script type="text/javascript" src="css/bootstrap/js/bootstrap.min.js"></script>
		<script type="text/javascript" src="js/parallax_js/parallax.min.js"></script>
		<script type="text/javascript" src="js/mainjQuery.js"></script>
	</body>
</html>
